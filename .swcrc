{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "dynamicImport": true,
      "decorators": true
    },
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true
    },
    "target": "es2022",
    "externalHelpers": false,
    "keepClassNames": true,
    "loose": false,
    "minify": {
      "compress": false,
      "mangle": false
    },
    "baseUrl": "src",
    "paths": {
      "@/*": ["*"],
      "@config": ["config"],
      "@interfaces/*": ["interfaces/*"],
      "@utils/*": ["utils/*"],
      "@constants/*": ["constants/*"],
      "@plugins/*": ["plugins/*"],
      "@components/*": ["components/*"],
      "@exceptions/*": ["exceptions/*"]
    }
  },
  "module": {
    "type": "commonjs"
  }
}
