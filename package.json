{
  "name": "fastify-typescript-starter",
  "version": "2.1.0",
  "description": "fastify + typescript starter",
  "main": "index.js",
  "keywords": [
    "node.js",
    "boilerplate",
    "fastify",
    "typescript",
    "prisma",
    "prettier",
    "eslint",
    "nodemon",
    "docker",
    "swagger",
    "swc"
  ],
  "scripts": {
    "start": "npm run build:prod && cross-env NODE_ENV=production node dist/server.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build:prod": "cross-env NODE_ENV=production swc src -d dist --source-maps --copy-files",
    "build:dev": "cross-env NODE_ENV=development swc src -d dist --source-maps --copy-files",
    "prisma:generate:dev": "dotenv -e .env.development.local -- npx prisma generate",
    "prisma:generate:prod": "dotenv -e .env.production.local -- npx prisma generate",
    "prisma:migrate:dev": "dotenv -e .env.development.local -- npx prisma migrate dev",
    "prisma:migrate:prod": "dotenv -e .env.production.local -- npx prisma migrate deploy",
    "prisma:seed:dev": "cross-env NODE_ENV=dev dotenv -e .env.development.local -- npx prisma db seed",
    "prisma:seed:prod": "cross-env NODE_ENV=prod dotenv -e .env.production.local -- npx prisma db seed",
    "prisma:reset:dev": "dotenv -e .env.development.local -- npx prisma migrate reset --skip-seed",
    "prisma:reset:prod": "dotenv -e .env.production.local -- npx prisma migrate reset --skip-seed",
    "check-types": "tsc --noemit",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/ && npm run check-types",
    "lint:fix": "npm run lint --fix",
    "prepare": "husky install"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma",
    "seed": "ts-node src/prisma/seed.ts"
  },
  "author": "TheB1gFatPanda",
  "license": "MIT",
  "dependencies": {
    "@fastify/compress": "6.4.0",
    "@fastify/cors": "8.3.0",
    "@fastify/env": "4.2.0",
    "@fastify/helmet": "11.0.0",
    "@fastify/jwt": "7.2.0",
    "@fastify/swagger": "8.8.0",
    "@fastify/swagger-ui": "1.9.3",
    "@fastify/type-provider-typebox": "3.4.0",
    "@prisma/client": "5.1.1",
    "@sinclair/typebox": "0.30.2",
    "ajv-errors": "3.0.0",
    "bcrypt": "5.1.0",
    "dotenv": "16.3.1",
    "dotenv-cli": "7.2.1",
    "fastify": "4.21.0",
    "fastify-plugin": "4.5.1"
  },
  "devDependencies": {
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.74",
    "@swc/helpers": "0.5.1",
    "@types/bcrypt": "5.0.0",
    "@types/node": "20.4.6",
    "@typescript-eslint/eslint-plugin": "6.2.1",
    "@typescript-eslint/parser": "6.2.1",
    "cross-env": "7.0.3",
    "eslint": "8.46.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-import": "2.28.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.3",
    "nodemon": "3.0.1",
    "prettier": "3.0.1",
    "prisma": "5.1.1",
    "source-map-support": "0.5.21",
    "ts-node": "10.9.1",
    "tsc-alias": "1.8.7",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.6"
  }
}
